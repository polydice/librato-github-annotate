{
  "name": "librato-github-annotate",
  "description": "Add deployment annotation to librato.",
  "repository": "https://github.com/polydice/librato-github-annotate",
  "env": {
    "LIBRATO_EMAIL": {
      "description": "Your librato email.",
      "generator": "secret"
    },
    "LIBRATO_API_KEY":  {
      "description": "Your librato API key.",
      "generator": "secret"
    },
    "ANNOTATION_NAME": {
      "description": "Metrix name in librato",
      "value": "deployment"
    },
    "PROCESS_COUNT": {
      "description": "Number of processes",
      "value": "3"
    }
  }
}
